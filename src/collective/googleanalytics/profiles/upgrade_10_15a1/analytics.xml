<?xml version="1.0"?>
<object name="portal_analytics" meta_type="Google Analytics Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="top-5-users-table" meta_type="Google Analytics Report"
    i18n:domain="collective.googleanalytics">
  <property name="title" i18n:translate="">Top 5 Users: Table</property>
  <property name="description"
     i18n:translate="">Displays a table of the top users by number of page views for this month.</property>
  <property name="categories">
   <element value="Site Wide"/>
  </property>
  <property name="plugin_names">
   <element value="Variable Date Range"/>
  </property>
  <property name="metrics">
   <element value="ga:pageviews"/>
  </property>
  <property name="dimensions">
   <element value="ga:customVarName3"/>
  </property>
  <property name="filters"/>
  <property name="sort">
   <element value="-ga:pageviews"/>
  </property>
  <property name="start_date"></property>
  <property name="end_date"></property>
  <property name="max_results">python:5</property>
  <property name="columns">python:['User name', 'Views']</property>
  <property name="row_repeat">python:dimension('ga:customVarName3')</property>
  <property
     name="rows">python:[row, metric('ga:pageviews', {'ga:customVarName3': row})]</property>
  <property name="viz_type">Table</property>
  <property name="viz_options"/>
  <property name="body">&lt;h3&gt;Top 5 Users&lt;/h3&gt;
   &lt;div tal:replace="structure view/visualization"&gt;&lt;/div&gt;</property>
 </object>
 <object name="top-users-table" meta_type="Google Analytics Report"
    i18n:domain="collective.googleanalytics">
  <property name="title" i18n:translate="">Top Users: Table</property>
  <property name="description"
     i18n:translate="">Displays a table of the top users by number of page views for this month.</property>
  <property name="categories">
   <element value="CSV"/>
  </property>
  <property name="plugin_names">
   <element value="Variable Date Range"/>
  </property>
  <property name="metrics">
   <element value="ga:pageviews"/>
  </property>
  <property name="dimensions">
   <element value="ga:customVarName3"/>
   <element value="ga:customVarValue3"/>
  </property>
  <property name="filters"/>
  <property name="sort">
   <element value="-ga:pageviews"/>
  </property>
  <property name="start_date"></property>
  <property name="end_date"></property>
  <property name="max_results"></property>
  <property name="columns">python:['Page Views', 'User name', 'Last access']</property>
  <property name="row_repeat">python:dimension('ga:customVarName3')</property>
  <property
     name="rows">python:[metric('ga:pageviews', {'ga:customVarName3': row}), row] + [max(dimension('ga:customVarValue3', {'ga:customVarName3': row}))]</property>
  <property name="viz_type">Table</property>
  <property name="viz_options"/>
  <property name="body">&lt;h3&gt;Top Users&lt;/h3&gt;
   &lt;div tal:replace="structure view/visualization"&gt;&lt;/div&gt;</property>
 </object>
